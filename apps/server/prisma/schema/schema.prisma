generator client {
  provider = "prisma-client"
  output   = "../generated"
  moduleFormat = "esm"
  runtime = "nodejs"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  email String @unique
  password String?
  name String
  googleId String? @unique
  provider String @default("local")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}